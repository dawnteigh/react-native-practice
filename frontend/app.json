{
  "expo": {
    "name": "dice-goblin-go",
    "slug": "dice-goblin-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Dice Goblin access to your photos?",
          "cameraPermission": "Allow Dice Goblin to access your camera?"
        }
      ]
    ],
    "icon": "./assets/dice-goblin-go-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/dice-goblin-go-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}